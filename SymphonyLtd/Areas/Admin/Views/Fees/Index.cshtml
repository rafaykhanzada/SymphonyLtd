<!--Custom Table-->
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    <strong>Striped</strong> Rows
                </h2>
                <ul class="header-dropdown m-r--5">
                    <li class="dropdown">
                        <a href="#" onclick="return false;" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            <i class="material-icons">more_vert</i>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li>
                                @Html.ActionLink("Create New", "Create", "Branches", new { area = "Admin" })
                            </li>
                            <li>
                                <a href="#" onclick="return false;">Refresh Data</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div class="body table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.EnrollmentID)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.StudentID)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.PaidAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.PaidDate)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ForMonth)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ChargeBy)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.tblFeesType.FeesType)
                            </th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.EnrollmentID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.StudentID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PaidAmount)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PaidDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ForMonth)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ChargeBy)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.tblFeesType.FeesType)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.FeesID }) |
                                @Html.ActionLink("Details", "Details", new { id = item.FeesID }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.FeesID })
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>